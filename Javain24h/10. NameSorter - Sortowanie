import java.util.Arrays;

public class NameSorter {
       public static void main (String[] args){
           String names[]= {"Glimer","Marvel", "Rue", "Clove", "Thresh", "Foxface", "Cato", "Peeta", "Katniss"};
           System.out.println(" Początkowa kolejność: ");
           for (int i=0; i<names.length; i++) {
             System.out.println(i+1 + " : " + names[i]);
           }

           Arrays.sort(names);
           System.out.println(" ");
           System.out.println("Nowa kolejność: ");
           for (int i=0; i<names.length; i++)
           {    System.out.println(i+1 + " : " + names[i]);}

           System.out.println();
       }

}
