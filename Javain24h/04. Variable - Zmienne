import java.util.Scanner;

public class Variable {
   public static void main (String[] args){
       int tops;
       float gradePointAverage;

       Scanner scanner = new Scanner(System.in);
       System.out.print("Podaj tops: ");
       tops = Integer.parseInt(scanner.nextLine());

       System.out.print("Podaj gradePointAverage: ");
       gradePointAverage = Float.parseFloat(scanner.nextLine());

       char key = 'C';
       String productName = "Larvets";

       System.out.println(tops + " " + gradePointAverage + " " + key + " " + productName );
   }
}
