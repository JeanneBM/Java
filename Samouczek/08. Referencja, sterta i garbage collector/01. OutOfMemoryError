/* 
Program, który spowoduje przepełnienie sterty i skończy się wyjątkiem java.lang.OutOfMemoryError.
*/

import java.util.ArrayList;

public class Memory {
    public static void main(String[] args) {
        ArrayList<Long> arrList = new ArrayList<>();

        while(true) {
            arrList.add(45L);
        }
    }
}
